CREATE DATABASE spdatarest;
USE spdatarest;



CREATE TABLE users
(
  id      BIGINT AUTO_INCREMENT
    PRIMARY KEY,
  email   VARCHAR(255) NULL,
  name    VARCHAR(255) NULL,
  surname VARCHAR(255) NULL
)
  ENGINE = InnoDB
  COLLATE = utf8_unicode_ci;




CREATE TABLE note
(
  id   BIGINT AUTO_INCREMENT
    PRIMARY KEY,
  name VARCHAR(255)  NULL,
  text VARCHAR(5000) NULL
)
  ENGINE = InnoDB;

CREATE TABLE users
(
  id      BIGINT AUTO_INCREMENT
    PRIMARY KEY,
  email   VARCHAR(255) NULL,
  name    VARCHAR(255) NULL,
  surname VARCHAR(255) NULL
)
  ENGINE = InnoDB
  COLLATE = utf8_unicode_ci;

